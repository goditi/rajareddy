# To Load IIS we must import module "webadministration"
Import-module webAdministration

#fetching the list of app-pools from IIS
#get-item IIS:\Apppools\* -force

#create new app-pool by using new-webapppool cmdlet
New-WebAppPool -Name "Devops"-Force -verbose

#loading particular App-pool "Devops" inta a veriable ,variable always starts with $